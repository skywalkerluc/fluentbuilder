{
    "name": "fluentbuilder",
    "version": "2.0.0",
    "description": "A fluent way to create builders for tests",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json --coverage",
        "cover": "jest --config jestconfig.json --ci --coverage && cat ./coverage/lcov.info | coveralls",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint --project tslint.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "postversion": "git push && git push --tags"
    },
    "author": "Lucas Teles",
    "keywords": [
        "Builder",
        "Fluent",
        "TS"
    ],
    "license": "MIT",
    "dependencies": {
        "@types/faker": "^4.1.5",
        "deprecated-decorator": "^0.1.6",
        "faker": "^4.1.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lucasteles/fluentbuilder"
    },
    "devDependencies": {
        "@types/jest": "^23.3.7",
        "coveralls": "^3.0.3",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.4",
        "tslint": "^5.11.0",
        "typescript": "^2.7.2"
    }
}